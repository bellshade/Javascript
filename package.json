{
  "name": "javascript",
  "version": "1.0.0",
  "description": "berisi informasi tentang implementasi dari javascript",
  "main": "viewer.js",
  "scripts": {
    "start": "node viewer.js",
    "lint": "eslint {learn,algorithm}/**/*.js --ignore-pattern node_modules",
    "prettier": "prettier --w learn/**/*{.js,.html} algorithm/**/*.js",
    "prettier-watch": "onchange 'learn/**/*{.js,.html} algorithm/**/*.js' -- prettier --w {{changed}}",
    "doctest-watch": "onchange '{learn,algorithm}/**/*.js' -- doctest {{changed}}"
  },
  "author": "bellshade",
  "license": "MIT",
  "devDependencies": {
    "doctest": "0.19.0",
    "eslint": "8.19.0",
    "onchange": "7.1.0",
    "prettier": "2.7.1"
  },
  "dependencies": {
    "@bellshade/ningali": "1.3.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@popperjs/core"
      ]
    }
  }
}
